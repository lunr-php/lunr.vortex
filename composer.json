{
  "name": "lunr/vortex",
  "description": "Lunr.Vortex is a set of notification dispatchers for various platforms.",
  "keywords": [ "notifications", "apns", "fcm", "wns", "jpush", "email" ],
  "homepage": "https://github.com/lunr-php/lunr.vortex",
  "type": "library",
  "license": "MIT",
  "require": {
    "psr/log": ">=3.0",
    "m2mobi/apns-php": ">=4.0",
    "phpmailer/phpmailer": "~6.4|~7.0",
    "rmccue/requests": "~2.0",
    "php": ">=8.3",
    "ext-xml": "*"
  },
  "require-dev": {
    "lunr/halo": "dev-master",
    "phpunit/phpunit": "~10.5",
    "mockery/mockery": "~1.5",
    "ext-xdebug": ">=3.1"
  },
  "config": {
    "audit": {
      "ignore": [
        "PKSA-z3gr-8qht-p93v"
      ]
    },
    "optimize-autoloader": true,
    "platform": {
        "php": "8.3.0"
    }
  },
  "autoload": {
    "psr-4": {"Lunr\\Vortex\\": "src/Lunr/Vortex/"}
  }
}
